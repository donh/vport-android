<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="ClearEditText">
        <!-- 清除按钮的图标 -->
        <attr name="iconClear" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SlideView">
        <attr name="maskText" format="string"/>
        <attr name="maskTextSize" format="dimension"/>
        <attr name="slider" format="reference"/>
        <attr name="sliderMarginLeft" format="dimension"/>
        <attr name="sliderMarginTop" format="dimension"/>
        <attr name="sliderbg" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ElegantNumberButton">
        <attr name="backGroundColor" format="color"/>
        <attr name="initialNumber" format="integer"/>
        <attr name="finalNumber" format="integer"/>
        <attr name="stepNumber" format="float"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="backgroundDrawable" format="reference"/>
    </declare-styleable>
</resources>
